{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Pet",
  "type": "object",
  "required": ["id", "name", "status"],
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": ["available", "pending", "sold"]
    },
    "photoUrls": {
      "type": "array",
      "items": {"type": "string"}
    }
  }
}
